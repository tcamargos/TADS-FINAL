create database blockchain;
----------------------------------------
create table atestados(id int auto_increment primary key not null, hash varchar(255), previoushash varchar(255),timestamp date,  nonce varchar(255), nome varchar(255), cpf varchar(255), crm varchar(255), cid varchar(255), dias varchar(255));
----------------------------------------
